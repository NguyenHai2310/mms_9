<%= form_for [:admin, @team] do |f| %>
  <%= render "shared/error_messages", object: f.object %>
    <table class="table">
      <tr>
        <th><%= t "team.user_in_team" %><th>
      </tr>
      <%= f.collection_check_boxes :user_ids, @users_in_team, :id, :username do |b| %>
        <tr>
          <%= b.label do %>
            <th><%= b.text %></th>
            <th class="selection_checkbox"><%= b.check_box %></th>
          <% end %>
        </tr>
      <% end %>
    </table>
  <%= f.submit t("team.deletemember"), class: "btn btn-primary" %>
<% end %>
